{
  "private": true,
  "name": "nr1-network-telemetry-nerdpack",
  "description": "Real time IPfix and Sflow visualization of network data passing through your system.",
  "version": "0.8.0",
  "scripts": {
    "checkversion": "check-node-version --node $(cat .nvmrc)",
    "format": "prettier --check \"**/*.js\" \"**/*.json\" \"**/*.jsx\" \"**/*.scss\" --ignore-path .eslintignore",
    "format:fix": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.jsx\" \"**/*.scss\" --ignore-path .eslintignore",
    "lint": "npm-run-all --parallel format lint:*",
    "lint:css": "stylelint 'nerdlets/**/*.scss' --syntax scss",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:js": "eslint --ext js,jsx --ignore-path .eslintignore .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "start": "nr1 nerdpack:serve",
    "test": "npm-run-all --sequential checkversion lint test:js",
    "test:js": "jest",
    "eslint-check": "eslint --ext js,jsx --ignore-path .eslintignore .",
    "eslint-fix": "npm run lint:js -- --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-push": "npm run test"
    }
  },
  "bugs": {
    "email": "opensource@newrelic.com"
  },
  "dependencies": {
    "@newrelic/nr1-community": "^1.2.0",
    "d3": "3.5.17",
    "lodash": "^4.17.20",
    "prop-types": "^15.6.2",
    "react-chord-diagram": "^1.5.0",
    "react-input-range": "^1.3.0",
    "react-vis": "^1.11.7",
    "semantic-ui-react": "^0.88.1"
  },
  "browserslist": [
    "last 2 versions",
    "not ie < 11",
    "not dead"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.1.0",
    "@newrelic/eslint-plugin-newrelic": "^0.3.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/experimental-utils": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-add-react-displayname": "0.0.5",
    "check-node-version": "^4.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-perf": "^3.2.4",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-webdriverio": "^1.0.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-scss": "^3.18.0",
    "ts-jest": "^24.1.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
